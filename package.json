{ "name": "teleport"
, "version": "0.2.7"
, "description": "CommonJS modules 1.0 loader for browsers."
, "homepage": "http://github.com/Gozala/teleport/"
, "keywords": ["commonjs", "modules", "require"]
, "contributors":
  [ "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)" ]
, "repository":
  { "type": "git"
  , "url": "git://github.com/Gozala/teleport.git"
  }
, "bugs": { "web": "http://github.com/Gozala/teleport/issues/" }
, "modules":
  { "utils/module": "./engines/common/utils/module"
  , "utils/package": "./engines/common/utils/package"
  }
, "overlay":
  { "node":
    { "dependencies":
      { "test": ">=0.0.10"
      , "system": ">=0.0.4"
      , "q": ">=0.1.5"
      , "promised-fs": ">=0.0.7"
      , "promised-utils": ">=0.0.8"
      , "markdown-js": ">=0.0.3"
      , "promised-http": ">=0.0.2"
      , "mustache": ">=0.3.0"
      , "backbone": ">=0.3.3"
      , "core-utils": ">=0.0.1"
      }
    , "directories":
      { "lib": "./engines/node"
      , "test": "./test"
      }
    }
  , "teleport":
    { "dependencies":
      { "markdown-js": ">=0.0.3"
      , "promised-http": ">=0.0.2"
      , "q": ">=0.1.5"
      , "mustache": ">=0.3.0"
      , "backbone": ">=0.3.3"
      }
    , "main": "./engines/teleport/teleport.js"
    , "directories": { "lib": "./engines/teleport" }
    }
  }
, "bin" : { "teleport" : "./engines/node/commands/commands.js" }
, "scripts": { "test": "node test/all.js" }
, "engines":
  { "node": ">=0.1.103"
  , "teleport": ">=0.2.0"
  }
, "licenses" :
  [ { "type" : "MPL 1.1/LGPL 2.1/GPL 2.0"
    , "url" : "http://www.mozilla.org/MPL/"
    }
  ]
}
